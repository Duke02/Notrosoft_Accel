<Window x:Class="Notrosoft_Accel.ImageViewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Notrosoft_Accel"
        mc:Ignorable="d"
        x:Name="mWind" Title="ImageViewWindow" Height="600" Width="800" SizeChanged="mWind_SizeChanged"
        Unloaded="mWind_Unloaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <Image x:Name="Graph1" Initialized="Graph1_Initialized" Stretch="UniformToFill" IsManipulationEnabled="True"
               HorizontalAlignment="Center" StretchDirection="DownOnly" />

    </Grid>
</Window>